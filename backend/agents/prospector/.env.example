# Prospector Agent Environment Variables

# Application
ENVIRONMENT=development
HOST=0.0.0.0
PORT=8001
LOG_LEVEL=INFO

# Database
DATABASE_URL=postgresql://postgres:postgres@localhost:5432/shabou_autopieces

# Redis
REDIS_URL=redis://localhost:6379/1

# Celery
CELERY_BROKER_URL=redis://localhost:6379/0
CELERY_RESULT_BACKEND=redis://localhost:6379/0

# Google Places API
GOOGLE_PLACES_API_KEY=your_google_places_api_key_here

# Scraping Settings
SCRAPING_TIMEOUT=30
SCRAPING_MAX_RETRIES=3

# Rate Limiting
GOOGLE_API_RATE_LIMIT=10
SCRAPING_RATE_LIMIT=5

# Email/SMTP Settings
SMTP_HOST=smtp.gmail.com
SMTP_PORT=587
SMTP_USER=your_email@gmail.com
SMTP_PASSWORD=your_app_password
SMTP_USE_TLS=true
FROM_EMAIL=prospector@shabou-autopieces.tn
FROM_NAME=Prospector Agent
REPORT_RECIPIENTS=owner@shabou-autopieces.tn,manager@shabou-autopieces.tn
